{
  "expo": {
    "name": "Dog character test App",
    "slug": "dog-character-test-app",
    "description": "You can add your dog's charactertest results in this app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D3D3D3"
    },
    "assetBundlePatterns": [
